# Environment Variables for ChitChat Frontend
# Copy this file to .env.local for local development

# Backend API URL (without /api suffix)
# For local development: http://localhost:8000/api
# For production: https://your-backend-project.deno.dev/api
VITE_API_URL=http://localhost:8000/api

# WebSocket URL (without token parameter)
# For local development: ws://localhost:8000
# For production: wss://your-backend-project.deno.dev
VITE_WS_URL=ws://localhost:8000
